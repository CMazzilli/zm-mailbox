<rules>
	<r name="test1">
		<c name="header" op=":contains" k0="From" k1="test1@filter.org"/>
		<action name="fileinto">
			<arg>/Drafts</arg>
		</action>
		<action name="stop"/>
	</r>
	<r name="test2">
		<c name="header" op=":contains" k0="To" k1="test2@filter.org"/>
		<action name="fileinto">
			<arg>/Drafts</arg>
		</action>
		<action name="stop"/>
	</r>
	<r name="test3">
		<c name="header" op=":contains" k0="cc" k1="test3@filter.org"/>
		<action name="fileinto">
			<arg>/Drafts</arg>
		</action>
		<action name="stop"/>
	</r>
	<r name="test4">
		<c name="header" op=":contains" k0="Subject" k1="test4"/>
		<action name="fileinto">
			<arg>/Drafts</arg>
		</action>
		<action name="stop"/>
	</r>
	<r name="test5">
		<g op="anyof">
			<c name="header" op=":contains" k0="From" k1="test5"/>
			<c name="header" op=":contains" k0="To" k1="test5"/>
			<c name="header" op=":contains" k0="Cc" k1="test5"/>
		</g>
		<action name="fileinto">
			<arg>/Drafts</arg>
		</action>
		<action name="stop"/>
	</r>
	<r name="test6">
		<g op="allof">
			<c name="header" op=":contains" k0="From" k1="test6"/>
			<c name="header" op=":contains" k0="Subject" k1="test6"/>
		</g>
		<action name="fileinto">
			<arg>/Drafts</arg>
		</action>
		<action name="stop"/>
	</r>
	<r name="test7">
		<c name="header" op=":contains" k0="sender" k1="test7@filter.org"/>
		<action name="fileinto">
			<arg>/Drafts</arg>
		</action>
		<action name="stop"/>
	</r>
	<r name="test13">
		<c name="attachment"/>
		<action name="tag">
			<arg>FilterTag3</arg>
		</action>
		<action name="stop"/>
	</r>
	<r name="test9">
		<g op="allof">
			<c name="date" op=":before" k1="12/1/2004"/>
			<c name="date" op=":after" k1="6/1/2004"/>
		</g>
		<action name="flag">
			<arg>flagged</arg>
		</action>
		<action name="stop"/>
	</r>
	<r name="test10">
		<c name="size" op=":over" k1="2048"/>
		<action name="flag">
			<arg>flagged</arg>
		</action>
		<action name="stop"/>
	</r>
	<r name="test11">
		<g op="allof">
			<c name="size" op=":over" k1="1024"/>
			<c name="size" op=":under" k1="2048"/>
		</g>
		<action name="flag">
			<arg>flagged</arg>
		</action>
		<action name="stop"/>
	</r>
	<r name="test12">
		<c name="body" op=":contains" k1="test12"/>
		<action name="flag">
			<arg>flagged</arg>
		</action>
		<action name="stop"/>
	</r>
	<r name="test14">
		<c name="addressbook" op=":in" k0="From" k1="contacts"/>
		<action name="flag">
			<arg>flagged</arg>
		</action>
		<action name="stop"/>
	</r>
	<r name="test16">
		<c name="header" op=":contains" k0="subject" k1="test16 action"/>
		<action name="fileinto">
			<arg>/Drafts</arg>
		</action>
		<action name="fileinto">
			<arg>/Spam</arg>
		</action>
		<action name="stop"/>
	</r>
	<r name="test17">
		<c name="header" op=":contains" k0="subject" k1="test17 action"/>
		<action name="keep"/>
		<action name="fileinto">
			<arg>/FilterFolder1</arg>
		</action>
		<action name="fileinto">
			<arg>/FilterFolder2</arg>
		</action>
		<action name="stop"/>
	</r>
	<r name="test18">
		<c name="header" op=":contains" k0="subject" k1="test18 action"/>
		<action name="tag">
			<arg>FilterTag1</arg>
		</action>
		<action name="tag">
			<arg>FilterTag2</arg>
		</action>
		<action name="stop"/>
	</r>
	<r name="test19">
		<c name="header" op=":contains" k0="subject" k1="test19 action"/>
		<action name="tag">
			<arg>FilterTag1</arg>
		</action>
		<action name="flag">
			<arg>read</arg>
		</action>
		<action name="tag">
			<arg>FilterTag2</arg>
		</action>
		<action name="flag">
			<arg>flagged</arg>
		</action>
		<action name="stop"/>
	</r>
	<r name="test20">
		<c name="header" op=":contains" k0="subject" k1="test20 action"/>
		<action name="tag">
			<arg>FilterTag1</arg>
		</action>
		<action name="fileinto">
			<arg>/FilterFolder1</arg>
		</action>
		<action name="tag">
			<arg>FilterTag2</arg>
		</action>
		<action name="fileinto">
			<arg>/FilterFolder2</arg>
		</action>
		<action name="tag">
			<arg>FilterTag3</arg>
		</action>
		<action name="stop"/>
	</r>
	<r name="test21">
		<c name="header" op=":contains" k0="subject" k1="test21 action"/>
		<action name="discard"/>
		<action name="fileinto">
			<arg>/FilterFolder1</arg>
		</action>
		<action name="fileinto">
			<arg>/FilterFolder2</arg>
		</action>
		<action name="keep"/>
		<action name="stop"/>
	</r>
	<r name="test22">
		<g op="anyof">
			<c name="header" op=":matches" k0="From" k1="*@test22.com"/>
			<c name="header" op=":matches" k0="To" k1="*@test22.com"/>
		</g>
		<action name="flag">
			<arg>flagged</arg>
		</action>
		<action name="stop"/>
	</r>
	<r name="test23">
		<g op="anyof">
			<c name="exists" k1="X-test23-0"/>
			<c name="exists" k1="X-test23-1"/>
		</g>
		<action name="fileinto">
			<arg>/Spam</arg>
		</action>
		<action name="stop"/>
	</r>
	<r name="test8">
		<c name="date" op=":before" k1="6/1/2004"/>
		<action name="flag">
			<arg>flagged</arg>
		</action>
		<action name="stop"/>
	</r>
	<r name="test24">
		<c name="header" op="not :contains" k0="From" k1="blah"/>
		<action name="discard"/>
		<action name="stop"/>
	</r>
</rules>
