<t:tests xmlns:t="urn:liquidTestHarness">

  <t:property name="uri" value="http://localhost:7070/service/soap/"/>

  <t:property name="user" value="user1"/>
  <t:property name="password" value="test123"/>

  <t:test id="auth" required="true">
    <t:request>
      <AuthRequest xmlns="urn:liquidAccount">
        <account by="name">${user}</account>
        <password>${password}</password>
        <!--<prefs/>-->
      </AuthRequest>
    </t:request>
    <t:response>
      <t:select path="//acct:AuthResponse/acct:lifetime"  match="^\d+$"/>
      <t:select path="//acct:AuthResponse/acct:authToken" set="authToken"/>
      <t:select path="//acct:AuthResponse/acct:sessionId" set="sessionId"/>
    </t:response>
  </t:test>

